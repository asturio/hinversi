#: human-cli/main.c:67
#, c-format
msgid "%s options:\n"
msgstr "%s opções:\n"

#: human-cli/main.c:68
#, c-format
msgid ""
"\t-b\t\tBLACK player [h0123] (default - h)\n"
"\t--black\n"
msgstr ""
"\t-b\t\tJogador PRETO [h0123] (padrão - h)\n"
"\t--black\n"

#: human-cli/main.c:69
#, c-format
msgid ""
"\t-w\t\tWHITE player [h0123] (default - 1)\n"
"\t--white\n"
msgstr ""
"\t-w\t\tJogador BRANCO [h0123] (padrão - 1)\n"
"\t--white\n"

#: human-cli/main.c:70
#, c-format
msgid ""
"\t-V\t\tDisplays the version number\n"
"\t--version\n"
msgstr ""
"\t-V\t\tMostra o número da versão\n"
"\t--version\n"

#: human-cli/main.c:71
#, c-format
msgid ""
"\t-h\t\tThis Help screen\n"
"\t--help\n"
msgstr ""
"\t-h\t\tEssa ajuda\n"
"\t--help\n"

